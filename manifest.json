{
  "name": "Spyder",
  "description": "",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["background/background.js"]
  },
  "browser_action": {
    "default_popup": "background/popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "128": "icons/128.png"
    }
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/16.png",
    "128": "icons/128.png"
  }
}
